<?php

/**
 * Implements hook_install().
 */
function dvg_webform_component_collections_uninstall() {
  $node_types = array('webform_collection');
  $nids = db_select('node', 'n')
    ->fields('n', array('nid'))
    ->condition('type', $node_types)
    ->execute()
    ->fetchCol();
  node_delete_multiple($nids);

  node_type_delete('webform_collection');
}